---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Vinod Kurup's Blog</title>
 <link href="http://www.kurup.org/blog/atom.xml" rel="self"/>
 <link href="http://www.kurup.org/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.kurup.org/blog/</id>
 <author>
   <name>Vinod Kurup</name>
   <email>vinod@kurup.com</email>
 </author>
 
 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.kurup.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
